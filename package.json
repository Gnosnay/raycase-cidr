{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cidr",
  "title": "CIDR",
  "description": "CIDR to IPv4 Conversion",
  "icon": "command-icon.png",
  "author": "Yansong",
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "cidr-to-ip-range",
      "title": "CIDR to IP Range",
      "description": "CIDR to IPv4 Conversion",
      "mode": "view",
      "arguments": [
        {
          "name": "cidrStr",
          "placeholder": "One valid CIDR string",
          "type": "text",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.51.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}